{
  "name": "suna-template",
  "packageManager": "pnpm@7.30.3",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "test": "turbo test",
    "test:watch": "turbo test:watch",
    "lint": "turbo lint",
    "hooks": "pnpm --filter=@suna/hooks",
    "utils": "pnpm --filter=@suna/utils",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "turbo": "^1.8.5"
  }
}
